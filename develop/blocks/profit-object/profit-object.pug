mixin profit-object(id, name, rate, libertyRate)
    +b.profit-object&attributes(attributes)
        +e.image
            img(src='images/redesign/object-cards/object' + id +'.jpg', alt="")
        +e.content
            +e.title Сколько получить
            +e.data
                +e.data-row
                    +e.data-value.__income
                        span.value(contenteditable='true') 0
                        span.suffix  ₽
                    +e.data-label Ежемесячный доход
                +e.data-row
                    +e.data-value.__share
                        span.value(contenteditable='true') 0
                        span.suffix &nbsp;
                    +e.data-label Приобретенных паев
                +e.data-row
                    +e.data-value.__step
                        span.value(contenteditable='true') 0
                        span.suffix  ₽
                    +e.data-label Стоимость пая
                +e.data-row
                    +e.data-value.__rate 
                        span.value(contenteditable='true')= rate
                        span.suffix %
                    +e.data-label Доходность
        +e.footer
            +e.name= name
            //- +e.A.link(href='#') Подробнее